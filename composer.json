{
	"name": "forumhulp/statistics",
	"type": "phpbb-extension",
	"description": "Board Statistics displays an overview of your user's visits in Quick Access module of ACP. This extension logs all visits of your users. A cron job will prune statistics.",
	"homepage": "http://forumhulp.com",
	"version": "3.1.2",
	"keywords": ["forumhulp", "phpbb", "extension", "statistics"],
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "John Peskens",
            "homepage": "http://forumhulp.com",
			"email": "info@forumhulp.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"phpbb/phpbb": "3.1.*@dev"
	},
	"extra": {
		"display-name": "Board Statistics",
		"soft-require": {
           "phpbb/phpbb": "3.1.*@dev"
        },
		"version-check": {
			"host": "forumhulp.com",
			"directory": "/ext",
			"filename": "index.php?id=32"
		}
	}
}
